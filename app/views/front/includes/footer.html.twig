        function handleResponse(html)
        {
            if (html.success)
            {
                alertify.success(html.data);
                
                (html.reload) ? window.location.reload() : '';

                (html.redirect) ? (window.location.href = html.redirect) : '';

            } else if (html.error) {
                alertify.error(html.error);
                return null;
            }
        }


